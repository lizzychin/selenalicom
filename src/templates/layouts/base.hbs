<!DOCTYPE html>
<html lang="en">
  <head>
    {{!-- head
    =================================== --}}
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>
      {{#isnt basename "index"}}{{title}} &middot; {{/isnt}}{{site.title}}
    </title>

    <!-- Custom CSS -->
    <link href="{{assets}}/css/elsa.min.css" rel="stylesheet">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="//oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="//oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>
  <body>
    {{!-- header
    =================================== --}}
    <header>
      <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
        <div class="container">

          <!-- Brand and toggle -->
          <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".navbar-collapse">
              <i class="fa fa-bars"></i>
            </button>
            <a class="navbar-brand" href="{{site.url}}">
              <img src="{{assets}}/images/logo.png" class="logo-dark" alt="Selena-Li.com" />
            </a>
          </div>

          <!-- Nav links -->
          <div class="collapse navbar-collapse navbar-right">
            <ul class="nav navbar-nav">
              <li{{#is basename 'about'}} class="active"{{/is}}>
                <a href="{{relative page.dest "about.html"}}">About Selena</a>
              </li>
              <li{{#is basename 'filmography'}} class="active"{{/is}}{{#is dirname "build/filmography"}} class="active"{{/is}}>
                <a href="{{relative page.dest "filmography.html"}}">Filmography</a>
              </li>
              <li{{#is basename 'beauty'}} class="active"{{/is}}{{#is dirname "build/beauty"}} class="active"{{/is}}>
                <a href="{{relative page.dest "beauty.html"}}">Beauty</a>
              </li>
              <li{{#is basename 'contact'}} class="active"{{/is}}>
                <a href="{{relative page.dest "contact.html"}}">Contact</a>
              </li>
            </ul>
          </div>

        </div>
      </nav>
    </header>


    {{> body }}


    {{!-- footer
    =================================== --}}
    <footer>
      <div class="container">
        <div class="row">
          <div class="copyright col-sm-7">
            <p>&copy; {{site.copyright}} <a href="{{site.url}}">{{site.title}}</a>.</p>
          </div>
          <div class="social-media col-sm-5">
            <ul class="list-inline">
              <li>
                <a href="http://facebook.com/{{site.selena.facebook}}" title="Selena Li's Facebook"><i class="fa fa-facebook"></i></a>
              </li>
              <li>
                <a href="http://instagram.com/{{site.selena.instagram}}" title="Selena Li's Instagram"><i class="fa fa-instagram"></i></a>
              </li>
              <li>
                <a href="http://youtube.com/user/{{site.selena.youtube}}" title="Selena Li's YouTube"><i class="fa fa-youtube"></i></a>
              </li>
              <li>
                <a href="http://www.weibo.com/u/{{site.selena.weibo}}" title="Selena Li's Sina Weibo"><i class="fa fa-weibo"></i></a>
              </li>
              <li>
                <a href="http://t.qq.com/{{site.selena.tencent}}" title="Selena Li's Tencent Weibo"><i class="fa fa-tencent-weibo"></i></a>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </footer>

    <a id="back-to-top">
      <i class="fa fa-angle-up"></i>
    </a>


    {{!-- foot
    =================================== --}}
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>

    {{#is basename "index"}}
      <script src="{{assets}}/js/plugins/instafeed.min.js"></script>
    {{/is}}
    {{#is basename "filmography"}}
      <script src="{{assets}}/js/plugins/isotope.min.js"></script>
      <script src="{{assets}}/js/plugins/imagesloaded.min.js"></script>
    {{/is}}
    {{#is basename "beauty"}}
      <script src="{{assets}}/js/plugins/isotope.min.js"></script>
      <script src="{{assets}}/js/plugins/imagesloaded.min.js"></script>
    {{/is}}

    <script src="{{assets}}/js/elsa.min.js"></script>

    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-12048611-1', 'auto');
      ga('send', 'pageview');
    </script>
  </body>
</html>